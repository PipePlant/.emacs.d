use warnings;
use strict;
use Encode;
use Data::Dumper;
use File::Basename;
use feature qw(say);

my %imgList = ();
map {$imgList{basename $_} = 1;} glob "./hdlg_dzyyqlx_resources/*";

my $img_data = "./tmp_data/img_log.txt";
open my $DATA ,"<", $img_data or die $!;
while (<$DATA>) {
    chomp;
    my $line = $_;
    $line =~ /\t(.*)/;
    my $data = $1;
    $data =~ s/\.jpeg$/\.jpg/;
    if (!exists $imgList{$data}) {
        say $line;
    }
}
close $DATA;
