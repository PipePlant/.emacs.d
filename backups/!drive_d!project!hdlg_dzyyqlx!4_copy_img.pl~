use warnings;
use strict;
use Encode;
use Data::Dumper;
use File::Basename;
use File::Find;
use feature qw(say);

my $data_file = "./tmp_data/hdlg_dzyyqlx_tmp_details.txt";

my @imgData = ();
open my $DATA ,"<", $data_file or die $!;
while (<$DATA>) {
    if ($_ =~ /"img":"(.+?)"/) {
        my $img = $1;
        $img =~ s/\.jpeg/\.jpg/;
        push @imgData,$img;
    }
}
close $DATA;

my @imgFile = glob "./OLD/original_data/image_rename/*";

