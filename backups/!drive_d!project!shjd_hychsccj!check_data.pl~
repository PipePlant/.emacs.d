use warnings;
use strict;
use Encode;
use Data::Dumper;
use JSON;
use feature qw(say);

my $data_file = "./final_data/shjd_hychsccj_details_full.txt";
open my $DATA ,"<", $data_file or die $!;

while (<$DATA>) {
    my $line = $_;

    if ($line =~ /\【/
        and $line !~ /\】/) {
        say $.."\t".$line;
    }
}

close $DATA;
