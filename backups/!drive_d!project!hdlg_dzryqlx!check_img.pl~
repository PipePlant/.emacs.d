use warnings;
use strict;
use Encode;
use Data::Dumper;
use File::Basename;
use feature qw(say);

my %imgList = ();
map {$imgList{basename $_} = 1;} glob "./hdlg_dzryqlx_resources/*";

my $img_data = "./tmp_data/img_log.txt";
open my $DATA ,"<", $img_data or die $!;
while (<$DATA>) {
    chomp;
    my $line = $_;
    $line =~ /\t(.*)/;
    if (!exists $imgList{$1}) {
        say $line;
    }
}
close $DATA;
